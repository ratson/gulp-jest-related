{
  "name": "gulp-jest-related",
  "version": "0.0.1",
  "description": "Gulp plugin for running Jest related tests.",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=lib/ src/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=lib/ src/",
    "clean": "rimraf lib dist coverage",
    "prepublish": "run-s clean build",
    "lint": "eslint --ext js --ext md .",
    "test": "npm run lint"
  },
  "dependencies": {
    "debug": "^2.6.0",
    "gulp-util": "^3.0.8",
    "jest-cli": "^18.1.0",
    "jest-util": "^18.1.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.14.1",
    "eslint-config-concise": "^0.6.1",
    "eslint-config-concise-esnext": "^0.6.0",
    "npm-run-all": "^4.0.1",
    "rimraf": "^2.5.4"
  },
  "repository": "ratson/gulp-jest-related",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ],
  "keywords": [
    "gulpplugin",
    "jest",
    "test",
    "testing",
    "gulp",
    "unit",
    "framework",
    "runner",
    "tdd",
    "bdd",
    "qunit",
    "spec",
    "tap"
  ]
}
